<script id="game-picture-template" type="text/x-handlebars-template">
  <div id="gamecard-container">
    {{#each this}}
  		<div class="gamecard" data-id="{{id}}" data-name="{{name}}">
  		  <img id="selected-character-{{@index}}">
  		  <div class="text-container center">
  		    <h5>{{name}}</h5>
  		  </div>
  		</div>
  	{{/each}}
  </div>
</script>

<script id="game-quote-template" type="text/x-handlebars-template">
    <h6 class="center">Quote {{counter}}/10</h6>
    <p>"{{content}}"</p>
</script>
